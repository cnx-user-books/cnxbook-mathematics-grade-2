            <image mime-type="image/png" src="../../media/graphics1-a934.png" id="id8706007__onlineimage" height="305" width="443"/>
          </media>
        </figure>
        <list id="id5486689" list-type="bulleted">
          <item>Draw the picture numbers for:</item>
        </list>
        <para id="id8839162">68 90</para>
